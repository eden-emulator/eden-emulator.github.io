{
  "name": "eden-emulator.github.io",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run build:blog && vite",
    "build:blog": "tsx scripts/build-blog.ts",
    "build": "npm run build:blog && tsc -b && vite build && cp CNAME dist/ && cp dist/index.html dist/404.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "types": "tsc -b --noEmit",
    "check": "npm run lint && npm run types",
    "check:fix": "npm run format && npm run lint:fix && npm run types",
    "preview": "vite preview",
    "watch:blog": "tsx watch scripts/build-blog.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.128.3",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.3.5",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "feed": "^5.1.0",
    "globals": "^16.3.0",
    "gray-matter": "^4.0.3",
    "prettier": "^3.6.2",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "unified": "^11.0.5",
    "vite": "^7.0.4"
  }
}
